.areaitem{
  width: 95%;
  height: 150rpx;
  margin: 0 auto;
}
.areaname{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  width: 73%;
  margin-left: 30rpx;
}
.unselected-binding {
  width: 22rpx;
  height: 22rpx;
  margin: 10rpx;
  position: absolute;
  top: 60rpx;
  right: 30rpx;
}
.add-room{
  color: #ffb34d
}
.add-room-icon{
  height: 40rpx;
  width: 40rpx;
  margin: 60rpx 30rpx 0 0;
  position: absolute;
  right: 0;
}
.save-box{
  position: fixed;
  top: 0;
  right: 0;
  background: #fff;
  z-index: 33;
  width: 100%;
}
.save-bind{
  width: 20%;
  float: right
}
